<scxml>
    <state id="s1">
        <onentry>
            <send type="basichttp" targetexpr="_ioprocessors['raw']['location']"
                  event="an_event">
                <param name="p1" expr="1" />
                <param name="p2" expr="2" />
            </send>
        </onentry>
        <transition event="*">
            <log label="caught raw basichttp" />
            <log label="input stream" expr="_event._input" />
            <log label="request method" expr="_event._method" />
        </transition>
    </state>
</scxml>